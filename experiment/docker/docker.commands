docker build -t atb00ker/test:ubuntu16.04 .
docker build -t atb00ker/test/openwisp:production .

docker network create --driver bridge --subnet 192.168.56.33/27 --gateway 192.168.56.33 -o "com.docker.network.bridge.name=docker-bridge" docker-bridge

docker run -dit --name "openwisp.production" --net docker-bridge --ip=192.168.56.34 atb00ker/test/openwisp:production
docker exec -it openwisp.production /bin/bash

ssh-keygen -R 192.168.56.34
ssh-keyscan -H openwisp.production >> ~/.ssh/known_hosts

NOTE(REQUIRED): apt install sshpass 
